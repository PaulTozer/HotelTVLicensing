# OpenAI API Key (required for AI extraction)
OPENAI_API_KEY=your_openai_api_key_here

# Optional: Azure OpenAI configuration
# AZURE_OPENAI_ENDPOINT=https://your-resource.openai.azure.com/
# AZURE_OPENAI_API_KEY=your_azure_key
# AZURE_OPENAI_DEPLOYMENT=gpt-4
# AZURE_OPENAI_FALLBACK_DEPLOYMENT=gpt-4.1-mini

# Optional: Azure Bing Search API (preferred over DuckDuckGo)
# AZURE_BING_SEARCH_ENDPOINT=https://api.bing.microsoft.com/v7.0/search
# AZURE_BING_SEARCH_KEY=your_bing_search_key

# Rate limiting
MAX_REQUESTS_PER_MINUTE=30
SCRAPE_TIMEOUT_SECONDS=30

# Logging
LOG_LEVEL=INFO
