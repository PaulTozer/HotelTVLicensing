# Azure OpenAI Configuration (required for AI extraction)
AZURE_OPENAI_ENDPOINT=https://your-resource.openai.azure.com/
AZURE_OPENAI_API_KEY=your_azure_openai_key
AZURE_OPENAI_DEPLOYMENT=gpt-4
AZURE_OPENAI_FALLBACK_DEPLOYMENT=gpt-4.1-mini

# Azure AI Foundry - Bing Grounding (primary search)
AZURE_AI_PROJECT_ENDPOINT=https://your-foundry-endpoint.services.ai.azure.com/api/projects/yourproject
AZURE_AI_MODEL_DEPLOYMENT_NAME=gpt-4.1-mini
BING_CONNECTION_NAME=your-bing-connection-name
USE_BING_GROUNDING=true

# Redis caching
# REDIS_URL=redis://localhost:6379
# REDIS_ENABLED=true
# CACHE_TTL_HOURS=24

# Rate limiting
MAX_REQUESTS_PER_MINUTE=30
SCRAPE_TIMEOUT_SECONDS=30

# Batch processing
# BATCH_MAX_CONCURRENT=25
# BATCH_MAX_SIZE=500
# BING_MAX_CONCURRENT=15
# BING_THREAD_POOL_SIZE=20
# BING_RETRY_MAX=3

# Logging
LOG_LEVEL=INFO
